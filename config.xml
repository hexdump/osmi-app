<?xml version='1.0' encoding='utf-8'?>
<widget id="com.osmi.osmi" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Osmi</name>
    <description>
      An application for administering olfactory sensitivity tests.
    </description>
    <author email="liamschumm@icloud.com" href="http://github.com/osmi-app/osmi-app">
        Liam Schumm
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="SplashScreen" value="none" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <icon src="assets/icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon height="180" src="assets/icon-60@3x.png" width="180" />
        <icon height="76" src="assets/icon-76.png" width="76" />
        <icon height="152" src="assets/icon-76@2x.png" width="152" />
        <icon height="57" src="assets/icon.png" width="57" />
        <icon height="114" src="assets/icon@2x.png" width="114" />
        <icon height="72" src="assets/icon-72.png" width="72" />
        <icon height="144" src="assets/icon-72@2x.png" width="144" />
        <icon height="29" src="assets/icon-small.png" width="29" />
        <icon height="58" src="assets/icon-small@2x.png" width="58" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.1.8" />
    <plugin name="nl.madebymark.share" spec="~0.1.1" />
</widget>
